    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.List;

List valueMetaList; 

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);

    if ( process ){
		String metadata = "";

        //Identifies the first row.
        if (first) {
            first = false;
			
			//Get the row metadata.
			valueMetaList = data.inputRowMeta.getValueMetaList();
        }

		//Init the metadata structure. 
		metadata = "[";

        //Process each row. 
        for (int i=0; i &lt; valueMetaList.size(); i++) {
			String object;

			//Get the field metadata. 
			ValueMetaInterface valueMeta = (ValueMetaInterface) valueMetaList.get(i);

			object = "{\"field\":\""; 
			object += 	valueMeta.getName();
			object += 	"\",\"type\":\""; 
			object += 	valueMeta.getTypeDesc(); 
			object += 	"\",\"length\":";
			object += 	Integer.toString( valueMeta.getLength() ); 
			object += 	",\"decimal\":"; 
			object += 	Integer.toString(  valueMeta.getPrecision() ); 
			object += "}";

			metadata = metadata + object;

			//Insert , as object separator. 
			if ( i &lt; ( valueMetaList.size() - 1 ) ){
				metadata = metadata + ",";
			}
		}

		//Close the metadata structure. 
		metadata = metadata + "]";

		//Creates the output row. 
		get(Fields.Out, "metadata").setValue(r, metadata );
        r = createOutputRow(r, data.outputRowMeta.size());

        //Send the row to the output port. 
        putRow(data.outputRowMeta, r);
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
      </definition>
