    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.regex.Pattern;
import java.util.regex.Matcher;

String[] type;
Pattern p;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);

    if( process ){
        //Identifies the first row.
        if (first) {
            first = false;

            //Identifies the data type of each input field;
            type = data.inputRowMeta.getFieldNamesAndTypes(0);

            //Identifies undesirable characteres and compile the regex.
			p = Pattern.compile("\n|\r|;|\\p{C}");
        }

        //Creates the output row. 
        Object[] output = createOutputRow(r, data.outputRowMeta.size());

        //Process each row. 
        for (int i=0; i &lt; type.length; i++) {
            //Remove the undesirable characteres from string fields.
            if ( ( type[i].trim() ).equals( "(String)" ) ) {
				if ( r[i] == null ) {
					output[i] = r[i];
				}else{
                	output[i] = p.matcher( (String) r[i] ).replaceAll("");
				}
            }else{
                output[i] = r[i];
            }			
        }
        
        //Send the row to the output port. 
        putRow(data.outputRowMeta, output);
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
      </definition>
