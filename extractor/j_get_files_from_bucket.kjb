      <script>mkdir -p ${RAWFILE_QUEUE_PATH}
mkdir -p ${RAWFILE_QUEUE_STAGING_PATH}

error_check(){
	if [ $? -gt 0 ]; then
       	 echo "An error has occurred :_("

		# Remove os arquivos temporÃ¡rios.
		if [ ${DEBUG} = 0 ] ; then
			rm -rf ${RAWFILE_QUEUE_STAGING_PATH}*
       	fi

		exit 1
	fi
}
	
echo "Reatrieving files from ${FILE_INPUT_BUCKET}"

if [ "${FILE_INPUT_BUCKET}" != "" ]; then
	# Identifies if the source is a S3 bucket. 
    if [[ "${FILE_INPUT_BUCKET}" == *"s3://"* ]]; then	
		aws s3 cp ${FILE_INPUT_BUCKET} ${RAWFILE_QUEUE_STAGING_PATH} --recursive --exclude "*" --include "*.${FILE_INPUT_EXTENSION}" --only-show-errors
		error_check
		
		FILE_COUNT=`ls  ${RAWFILE_QUEUE_STAGING_PATH} | wc -l`
    else
		FILE_COUNT=`ls  ${FILE_INPUT_BUCKET} | wc -l`
	fi

	echo "${FILE_COUNT} files found"

	#Identifies if there are files to process.
	if [ ${FILE_COUNT} -gt 0 ]; then
		cp ${FILE_INPUT_BUCKET}* ${RAWFILE_QUEUE_STAGING_PATH}
		error_check	
	
		#Identifies if FILE_OUTPUT_MODE is APPEND. 
		if [[ ${FILE_OUTPUT_MODE} == "append" ]]; then	
			#Identifies if should change the file extension to CSV. 
			if [ ${FILE_INPUT_EXTENSION} != "csv" ]; then
				for i in `ls ${RAWFILE_QUEUE_STAGING_PATH}*`
				do
					mv "$i" "${i%.*}.csv" 
				done
			fi
		fi
	
		#Identifies if files should be merged. 
		if [[ "${TARGET}" == "spectrum" ]] ; then
				MERGE_REQUIRED=0
		else
				echo "Merge is required for ${TARGET}"
				MERGE_REQUIRED=1
		fi
	
		FILE_INDEX=0
	
		for i in `ls ${RAWFILE_QUEUE_STAGING_PATH}*`
		do
			if [ -f "${i}" ]; then
				if [[ ${FILE_OUTPUT_MODE} == "append" &amp;&amp; ${FILE_INPUT_PARTITONED} -eq 0 &amp;&amp; ${MERGE_REQUIRED} -eq 0  ]]; then	
					echo "Copying file ${i} to ${RAWFILE_QUEUE_PATH}"
					mv ${i}	${RAWFILE_QUEUE_PATH}${SCHEMA_NAME}_${TABLE_NAME}_$(basename "${i}")
					error_check			
				else
					if [ ${FILE_INDEX} = 0 ]; then	
						cat ${i} >>	${RAWFILE_QUEUE_FILE}
						error_check
					else
						if [[ ${FILE_INPUT_PARTITONED} -eq 1 &amp;&amp; ${MERGE_REQUIRED} -eq 0 ]]; then
							mv ${i}	${RAWFILE_QUEUE_PATH}${SCHEMA_NAME}_${TABLE_NAME}_${FILE_INDEX}.csv
							error_check
						else
							echo "Merging file ${i} in ${RAWFILE_QUEUE_FILE}"
							sed '1d' ${i} >> ${RAWFILE_QUEUE_FILE}	
							error_check	
						fi
					fi

					FILE_INDEX=$(( $FILE_INDEX + 1 ))
				fi
			fi
		done	
	else
		echo "There are no files in ${FILE_INPUT_BUCKET} :/"
	fi	
	
	#Remove stage folder.
	rm -rf ${RAWFILE_QUEUE_STAGING_PATH}
else 
    echo "Input bucket not provided :/"
fi</script>
