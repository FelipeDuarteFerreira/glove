      <script>mkdir -p ${METADATA_QUEUE_PATH}
mkdir -p ${RAWFILE_QUEUE_PATH}

error_check(){
	if [ $? -gt 0 ]; then
       	 echo "An error has occurred :_("

        	# Remove os arquivos tempor√°rios.
        	if [ ${DEBUG} = 0 ] ; then
            		rm -rf ${RAWFILE_QUEUE_PATH}*
       	 fi

		exit 1
	fi
}

if [ ${DEBUG} = 1 ] ; then
	echo  "${INPUT_DATA_STATEMENT_ESCAPED}"
fi

QUERY_ID=$(aws athena start-query-execution --query-string  "${INPUT_DATA_STATEMENT_ESCAPED}"  --output text --result-configuration OutputLocation=s3://${STORARE_BUCKET_STAGING}/staging)
error_check

echo "Running query $QUERY_ID!"

while true
do
    QUERY_STATUS=`aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.Status.State' | sed 's/\"//g'`
    error_check

    if [[ "$QUERY_STATUS" == "SUCCEEDED" ]]; then
       QUERY_RESULTSET=`aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.ResultConfiguration.OutputLocation' | sed 's/\"//g'`
	error_check

	echo "Query resultset at $QUERY_RESULTSET!"
	break
    elif [[ $QUERY_STATUS == "FAILED" || $QUERY_STATUS == "CANCELLED" ]]; then
       echo "Query  $QUERY_ID failed!"
	echo `aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.Status.StateChangeReason'`
	break
    else
        sleep 10
    fi
done

if [ -z $QUERY_RESULTSET ]; then
	exit 1
else
	echo "Synchronizing Amazon S3 Buckets"
	sleep 15

	echo "Copyng resultset from $QUERY_RESULTSET to ${RAWFILE_QUEUE_PATH}!"
    	aws s3 cp $QUERY_RESULTSET ${RAWFILE_QUEUE_PATH} --only-show-errors

	echo "Renaming file ${RAWFILE_QUEUE_PATH}$QUERY_ID.csv to ${RAWFILE_QUEUE_FILE}"
   	mv ${RAWFILE_QUEUE_PATH}$QUERY_ID.csv ${RAWFILE_QUEUE_FILE}
fi
</script>
