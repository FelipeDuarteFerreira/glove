      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.text.SimpleDateFormat;
import java.util.Date;
import org.apache.commons.lang.time.DateUtils;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);

    if( process ){
		//Get variables. 
		String order = get(Fields.In, "order").getString(r);
		String step_parameter = get(Fields.In, "parameter").getString(r);
		String generic_parameter = getVariable("GENERIC_PARAMETER", "");
		String sql_in_one_line = get(Fields.Out, "sql_in_one_line").getString(r);
		String[] global_parameters = new String[0];

		//Identify parameter scope. 
        if( step_parameter == null || step_parameter.isEmpty() ){
			step_parameter = generic_parameter;  
        } else {
			//Parser global parameters. 
			global_parameters = generic_parameter.split("\\|");
		}

		if( ! step_parameter.isEmpty() ){
            //Parser step parameters. 
            String[] parameters = step_parameter.split("\\|");

            for (int i = 0; i &lt; parameters.length; i++) {
                //Replace first : to |.
				parameters[i] = parameters[i].replaceFirst(":", "|");

                //Get parameter value. 
                String[] values = parameters[i].split("\\|");

                for (int j = 0; j &lt; global_parameters.length; j++) {
					//Replace first : to |.
                    global_parameters[j] = global_parameters[j].replaceFirst(":", "|");

                    //Get parameter value. 
                    String[] global_values = global_parameters[j].split("\\|");

                    //Identify if should use a global parameter. 
                    if (global_values[0].contains(order + "." + values[0])) {
                        values[1] = global_values[1];
                    }
                }

				if ( values.length == 2 ) {
					//Identify if a parameter is a date macro (#). 
					if ( values[1].startsWith("#") ){
						String mask;
						String[] date_values = values[1].split(",");

						//Get date mask.
						if ( date_values.length == 2 ){
							mask = date_values[1];
						} else {
							mask = "yyyy-MM-dd";
						}

						//Get date macro. 
						String macro = date_values[0].replace( "#", "" );

						//Identify the data operation. 
						if ( macro.contains( "-" ) ) {
							String[] operation = macro.split("-");
							values[1] = new SimpleDateFormat(mask).format( new DateUtils().addDays(new Date(), ( Integer.parseInt( operation[1]) ) * -1 ) );

						} else if ( macro.contains( "+" ) ) {
							String[] operation = macro.split("\\+");
							values[1] = new SimpleDateFormat(mask).format( new DateUtils().addDays(new Date(), Integer.parseInt( operation[1]) ) );

						} else {
							values[1] = new SimpleDateFormat(mask).format( new Date() );
						}	

						logBasic( "Result of data macro ( " + macro + " ) evaluation of parameter " + values[0] + ": " + values[1] );				
					}

					//Replace query parameters.
					sql_in_one_line = sql_in_one_line.replaceAll( "\\$\\{" + values[0].toUpperCase() + "\\}", values[1] );	
				}else if ( values.length == 1 ){
                    sql_in_one_line = sql_in_one_line.replaceAll( "\\$\\{" + values[0].toUpperCase() + "\\}", "" );	    
                } 		       		
        	}				
		} 

		//Update field value. 
		get(Fields.Out, "sql_in_one_line").setValue(r, sql_in_one_line);

        //Create output row. 
        Object[] output = createOutputRow(r, data.outputRowMeta.size());

        //Send the row to the output port. 
        putRow(data.outputRowMeta, output);
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
      </definition>
