        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);

    if( process ){
		//Get ignored step.
		String ignored_step = get(Fields.In, "ignored_step").getString(r);
		String[] steps = new String[0]; 

		//Identify if are step to ignore. 
		if( ! ignored_step.isEmpty() ){
			//Split steps. 
			steps = ignored_step.split(",");

			for ( int i = 0; i &lt; steps.length; i++ ) {
				//Identify if it is a range. 
				if ( steps[i].contains("-") ){
					String[] range = steps[i].split("-");

					if ( range.length == 2 ) {	
						//Evaluate the range and send the step to the output. 
						for ( int j = Integer.valueOf(range[0]); j &lt;= Integer.valueOf(range[1]); j++ ) {
							Object[] out = createOutputRow(r, data.outputRowMeta.size());
							get(Fields.Out, "step_to_ignore").setValue( out, Integer.toString(j) );
							putRow(data.outputRowMeta, out);
						}	
					}	
				} else {
					//Send the step to the output. 
					Object[] out = createOutputRow(r, data.outputRowMeta.size());
					get(Fields.Out, "step_to_ignore").setValue( out, steps[i].toString() );
					putRow(data.outputRowMeta, out);
				}
			}
		}
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
